{
    "name": "trg-recruits-backend",
    "version": "1.0.0",
    "description": "",
    "main": "server.js",
    "scripts": {
        "start": "DOTENV_CONFIG_PATH=.env node -r dotenv/config ./build/server.js",
        "start:debug": "DEBUG=express:* DOTENV_CONFIG_PATH=.env node -r dotenv/config ./build/server.js",
        "start:inspect": "DOTENV_CONFIG_PATH=.env node -r dotenv/config --inspect ./build/server.js",
        "dev": "cross-env DOTENV_CONFIG_PATH=.env ts-node -r dotenv/config ./src/server.ts",
        "dev:watch": "nodemon --config nodemon.json",
        "clean": "rimraf ./build && rm ./build/src/**/*.map",
        "build": "npm run lint && npm run clean && tsc",
        "lint": "",
        "build:watch": "tsc -w",
        "test": "jest --coverage",
        "test:watch": "jest --watch",
        "db:migrate": "sequelize-cli db:migrate",
        "db:seed:all": "sequelize-cli db:seed:all",
        "prettier": "lint-staged",
        "precommit": "npm run prettier && npm run lint",
        "prepush": "npm run build",
        "format": "prettier --write ."
    },
    "author": "Abhirami",
    "license": "ISC",
    "dependencies": {
        "aws-sdk": "^2.1281.0",
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.20.1",
        "cors": "^2.8.5",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "express-validator": "^6.14.2",
        "helmet": "^6.0.1",
        "http-status-codes": "^2.2.0",
        "install": "^0.13.0",
        "joi": "^17.7.0",
        "jsonwebtoken": "^9.0.0",
        "jwt-decode": "^3.1.2",
        "multer": "^1.4.5-lts.1",
        "multer-s3": "^3.0.1",
        "mysql": "^2.18.1",
        "mysql2": "^2.3.3",
        "nodemailer": "^6.8.0",
        "passport": "^0.6.0",
        "passport-json": "^1.2.0",
        "passport-local": "^1.0.0",
        "pg": "^8.8.0",
        "postgres": "^3.3.2",
        "rimraf": "^3.0.2",
        "sequelize": "^6.28.0",
        "typescript": "^4.9.4",
        "winston": "^3.8.2"
    },
    "devDependencies": {
        "@types/cors": "^2.8.13",
        "@types/express": "^4.17.15",
        "@types/node": "^18.11.17",
        "@typescript-eslint/eslint-plugin": "^5.47.0",
        "@typescript-eslint/parser": "^5.47.0",
        "cross-env": "^7.0.3",
        "eslint": "^8.30.0",
        "eslint-config-google": "^0.14.0",
        "nodemon": "^2.0.20",
        "prettier-eslint": "^15.0.1",
        "sequelize-cli": "^6.5.2",
        "ts-node": "^10.9.1",
        "tslint-config-prettier": "^1.18.0"
    }
}